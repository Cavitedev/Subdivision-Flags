{
    "name": "Osu Subdivide Nations",
    "version": "0.1.0",
    "description": "Updates national flags into regional flags in Osu! website",
    "host_permissions": [
        "https://osu.ppy.sh/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "resources": [
                "local_user_regions.js",
                "flags/ES.json"
            ]
        }
    ],
    "manifest_version": 3
}