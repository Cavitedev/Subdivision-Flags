{
    "name": "osu! subdivide nations",
    "version": "3.0.2",
    "description": "Updates national flags into regional flags on the osu! website.\n While Also updating osu! global score on profiles",
    "browser_specific_settings": {
        "gecko": {
            "id": "cavitedev@gmail.com",
            "strict_min_version": "109.0"
        }
    },
    "permissions": [
        "tabs",
        "storage"
    ],  
    "host_permissions": [
        "https://osuworld.octo.moe/api/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "js": [
                "content-script/osu/content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "resources": [
                "flags.json"
            ]
        }
    ],
    "icons": {
        "128": "assets/favicon/icon128.png",
        "48": "assets/favicon/icon48.png",
        "32": "assets/favicon/icon32.png",
        "16": "assets/favicon/icon16.png"
    },
    "action": {
        "default_popup": "ui/popup/popup.html",
        "default_icon": {
            "128": "assets/favicon/icon128.png",
            "48": "assets/favicon/icon48.png",
            "32": "assets/favicon/icon32.png",
            "16": "assets/favicon/icon16.png"
        }
    },
    "manifest_version": 3
}