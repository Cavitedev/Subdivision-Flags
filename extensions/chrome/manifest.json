{
    "name": "osu! subdivide nations",
    "version": "2.3.1",
    "description": "Updates national flags into regional flags in Osu! website.\n Flags from Wikimedia Commons.",
    "permissions": ["tabs"],

    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "https://osu.ppy.sh/*"
            ],
            "resources": [
                "flags.json"
            ]
        }
    ],
    "manifest_version": 3
}